generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String  @id @default(uuid())
  googleId  String? @unique
  facebookId String? @unique
  email     String  @unique
  name      String?
  picture   String?
  createdAt DateTime @default(now())
}

